--
--  compile to JSON with 'iotql-model Insteon.iotql'
--
CREATE MODEL Insteon WITH
    schema:name = "Insteon",
    schema:description = "Insteon"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
