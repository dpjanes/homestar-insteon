--
--  compile to JSON with 'iotql-model InsteonLight.iotql'
--
CREATE MODEL InsteonLight WITH
    schema:name = "Insteon Light",
    schema:description = "Insteon Light"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
